{
    "manifest_version": 3,
    "name": "Company Notice Board",
    "version": "1.0",
    "description": "A notice board with widgets for the chrome homepage.",
    "action": {
      "default_popup": "index.html"
    },
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }
  